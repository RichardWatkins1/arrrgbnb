<h1> New listing </h1>

<form action='/property/all' method='POST'>
  <label for='title'>
    Title:
    <input type='text' name='title' />
  </label>
  <label for='property_type'>
    Property Type:
    <input type='text' name='property_type' />
  </label>
  <label for='location'>
    Location:
    <input type='text' name='location' />
  </label>
  <label for='bedrooms'>
    Bedrooms:
    <input type='text' name='bedrooms' />
  </label>
  <label for='sleeps'>
    Sleeps:
    <input type='text' name='sleeps' />
  </label>
  <label for='photo'>
    Photo:
    <input type='text' name='photo' />
  </label>
  <label for='price'>
    Price:
    <input type='text' name='price' />
  </label>

  <input type='submit' value='Create listing' />

</form>
